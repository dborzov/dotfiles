# gitconfig

[user]
    name = Dmitry Borzov
    email = tihoutrom@gmail.com

[alias]
    lg  = log --graph --decorate
    llg = log --oneline --graph --decorate
    ll  = log --oneline
    ls  = show --pretty="" --name-only
    dc = diff --cached
    st = status --short
    ci = commit
    rb = rebase
    br = branch
    co = checkout
